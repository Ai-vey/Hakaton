<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Loaded="Window_Loaded"  >
    <Grid>
        <StackPanel VerticalAlignment="Top" HorizontalAlignment="Left">
            <Button Margin="15" Name="BtnBack" Height="30" Width="60" Content="back" Click="BtnBack_Click"/>
        </StackPanel>
        <Frame x:Name="MainFrame" Navigated="MainFrame_Navigated" NavigationUIVisibility="Hidden" ContentRendered="MainFrame_ContentRendered_1" Grid.Column="0"/>
    </Grid>
</Window>
